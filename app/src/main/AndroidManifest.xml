<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eunbi.homein.homeinproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Menu.MyInfoActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Menu.NoticeActivity" />
        <activity android:name=".Menu.InformationRuleActivity" />
        <activity android:name=".Menu.ServiceInfoActivity" />
        <activity android:name=".Menu.SettingActivity" />
        <activity android:name=".Menu.VersionInfoActivity" />
        <activity android:name=".HomeIn.PeopleItemDetailActivity" />
        <activity
            android:name=".Company.CompanyInfoActivity"
            android:label="@string/title_activity_company_info"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
